class Dog extends Mammal {
            public Dog(String name) {
                super(name);    }
            public void greats() {
                System.out.println("WOOF");    }
            public void greats(Dog another) {
                System.out.println("WOOOOF");    }
            public String toString() {
                return "Dog: Dog[" + super.toString() + "]";
            }
        }
